<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://openprovenance.org/model/opmPrinterConfig">

  <graphName>OPMGraph</graphName>


  <activities displayValue="true">
    <activity value="http://activity.org/add1ToAll" display="add1ToAll"/>
    <activity value="http://activity.org/split"     display="split"/>
    <activity value="http://activity.org/plus1"     display="+1"/>
    <activity value="http://activity.org/cons"      display="cons"/>
    <activity value="http://activity.org/fry"       display="fry"/>
    <activity value="http://activity.org/bake"      display="bake"/>
    <activity value="http://activity.org/badBake"   display="badBake"/>
  </activities>

  <entities displayValue="true">
    <entity value="2 eggs"       display="two eggs"/>
  </entities>

  <agents displayValue="true">
  </agents>


  <relations default="filled">
    <relation type="org.openprovenance.prov.xml.Used"            style="dotted"  printRole="true"/>
    <relation type="org.openprovenance.prov.xml.WasGeneratedBy"  style="dotted"  printRole="true"/>
    <relation type="org.openprovenance.prov.xml.WasControlledBy" style="dotted"  printRole="true"/>
    <relation type="org.openprovenance.prov.xml.WasDerivedFrom"  style="bold"/>
  </relations>


  <accounts defaultAccount="black" defaultColor="black">
    <account name="orange"      color="red"/>
  </accounts>


</configuration>  
