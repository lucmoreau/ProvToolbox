					"type": "string"
				},
				"@language": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"ArrayOfValues": {
			"$id": "#/definitions/ArrayOfValues",
			"type": "array",
			"items": {
				"anyOf": [
					{ "$ref": "#/definitions/QualifiedName" },
					{ "$ref": "#/definitions/typed_value" },
					{ "$ref": "#/definitions/lang_string" }
				]
