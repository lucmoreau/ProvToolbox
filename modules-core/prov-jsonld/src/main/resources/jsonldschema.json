		},
		"ArrayOfLabelValues": {
			"$id": "#/definitions/ArrayOfLabelValues",
			"type": "array",
			"items": { "$ref": "#/definitions/lang_string" }
		},
		"Context": {
			"$id": "#/definitions/Context",
			"type": "array",
			"title": "The @context Schema",
			"items": {
				"oneOf": [
					{
						"type": "string",
						"format": "uri"

