
{

	"select": {"decision":       {"@type": "prov:Entity"}},

	"query": [
		"prefix plead <https://plead-project.org/ns/plead#>",
		"prefix sk <https://scikit-learn.org/stable/modules/generated/sklearn.>",



		"select * from der1 a prov:WasDerivedFrom",
		"from file2 a prov:Entity",
		" join der1.generatedEntity=file2.id",
		"from file1 a prov:Entity",
		" join der1.usedEntity=file1.id",
		"from act0 a prov:Activity",
		" join der1.activity=act0.id",

		"from wat1 a prov:WasAttributedTo",
		" join file2.id=wat1.entity",
		"from engineer a prov:Agent",
		" join wat1.agent = engineer.id",

		"from aobo1 a prov:ActedOnBehalfOf",
		" ljoin engineer.id=aobo1.delegate",




		"where file2[prov:type]    >= 'plead:DataFile'",
		"  or  file2[prov:type]    >= 'plead:AccuracyScore'",
		"  or  file2[prov:type]    >= 'sk:Pipeline'",

		"group by file2, engineer, act0 aggregate file1, aobo1 with Seq"

	],

	"sentence": {
		"type": "clause",
		"front-modifiers": [
			{
				"type": "preposition_phrase",
				"preposition": "on behalf of",
				"noun": {
					"type": "coordinated_phrase",
					"conjunction": "and",
					"@iterator": {
						"type": "@iterator",
						"@variable": "aobo1",
						"@clause": "coordinates",
						"@element": {
							"type": "@funcall",
							"@object": "aobo1",
							"@field": "responsible",
							"@function": "noun+identity",
							"@arg1": "agent"
						}
					}
				}
			}
		],
		"subject": {
			"type": "noun_phrase",
			"determiner": "the",
			"head": {
				"type": "@funcall",
				"@object": "engineer",
				"@field": "id",
				"@function": "noun+identity",
				"@arg1": "engineer"
			}
		},
		"object": {
			"type": "coordinated_phrase",
			"conjunction": "and",
			"@iterator": {
				"type": "@iterator",
				"@variable": "file1",
				"@clause": "coordinates",
				"@element": {
					"type": "@funcall",
					"@object": "file1",
					"@field": "id",
					"@function": "noun+identity",
					"@arg1": "file"
				}
			}
		},
		"complements": [
			{
				"type": "preposition_phrase",
				"preposition": "into",
				"noun": {
					"type": "@funcall",
					"@object": "file2",
					"@property":  "prov:type",
					"@function": "lookup-type",
					"@arg1": "noun_phrase",
					"@arg2": "plead,sk",
					"post-modifiers": [
						{
							"type":"adjective_phrase",
							"head": {
								"type": "@funcall",
								"@object": "file2",
								"@field":  "id",
								"@function": "noun+identity",
								"@arg1": ""
							}
						},
						{
							"type": "preposition_phrase",
							"preposition": "with",
							"noun": {
								"type": "@funcall",
								"@object": "file2",
								"@property": "plead:filename",
								"@function": "noun+identity",
								"@arg1": "name",
								"@optional": "true"
							}
						},
						{
							"type": "preposition_phrase",
							"preposition": "with",
							"noun": {
								"type": "@funcall",
								"@object": "file2",
								"@property": "prov:value",
								"@function": "noun+identity",
								"@arg1": "value",
								"@optional": "true"
							}
						}

					]
				}
			}
		],

		"verb": {
			"type": "@funcall",
			"@object": "act0",
			"@property":  "prov:type",
			"@function": "lookup-type",
			"@arg1": "verb_phrase",
			"@arg2": "plead"
		},

		"features" : {
			"tense": "past",
			"passive": "false"
		}

	},


	"context": {
		"ex": "http://example.org/#",
		"foaf": "http://xmlns.com/foaf/0.1/",
		"plead": "https://plead-project.org/ns/plead#",
		"sk": "https://scikit-learn.org/stable/modules/generated/sklearn."
	}


}
