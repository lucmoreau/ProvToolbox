			"preposition": "on",
			"noun": {
				"type": "@funcall",
				"@object": "wib",
				"@property": "ln:at",
				"@optional": "true",
				"@function": "timestring"}
		},
		"verb": "delete",

		"modifiers": [
			{
				"type":  "noun_phrase",
				"head":  ":"
			},
			{
				"type": "coordinated_phrase",
				"conjunction": "and",
				"@iterator": {
					"type": "@iterator",
					"@variable": "application",
					"@clause": "coordinates",
					"@element": {
						"type": "@funcall",
						"@object": "application",
						"@property": "prov:type",
						"@function": "lookup-type",
						"@arg1": "noun_phrase",
						"@arg2": "ln",
						"post-modifiers": [
							{
								"type": "adjective_phrase",
								"head": {
									"type": "@funcall",
									"@object": "application",
									"@field":  "id",
