
{

    "select": {"ag": {"@type": "prov:Agent"}},

    "query": [

	"prefix foaf <http://xmlns.com/foaf/0.1/>",
	"select * from ag a prov:Agent ",
	"where ag[prov:type] >= 'prov:Person'",
        " and ag[foaf:givenName] exists"],



