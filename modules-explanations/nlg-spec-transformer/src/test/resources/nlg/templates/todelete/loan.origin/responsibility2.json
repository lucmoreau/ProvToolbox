					  "@arg1": "agent"}}},
		     {"type": "preposition_phrase",
		       "preposition": "for",
		       "noun":  {"type":"noun_phrase",
				 "head": {"@object": "do",
					  "@field":  "id",
					  "@function": "noun+localname",
					  "@arg1": "approval"}}},
		     {"type": "preposition_phrase",
		       "preposition": "with",
		       "noun":  {"type":"noun_phrase",
				 "determiner": "the",
				 "head": {"@object": "actor",
					  "@field":  "id",
					  "@function": "noun+localname",
					  "@arg1": "agent"}}},
		     {"type": "preposition_phrase",
		       "preposition": "for",
		       "noun":  {"type":"noun_phrase",
				 "head": {"@object": "do",
					  "@field":  "id",
					  "@function": "noun+localname",
