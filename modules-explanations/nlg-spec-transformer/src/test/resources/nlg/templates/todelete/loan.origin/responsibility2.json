{
    "select": {"recommendation": {"@type": "prov:Entity"},

	       "pipeline2":      {"@type": "prov:Entity"},
	       "actor2":         {"@type": "prov:Agent"},
	       "do2":            {"@type": "prov:Activity"},	       
	       "wdf2":           {"@type": "provext:WasDerivedFromPlus"},
	       "wgb2":           {"@type": "prov:WasGeneratedBy"},	       
	       "waw2":           {"@type": "prov:WasAssociatedWith"},
	       
	       "pipeline3":      {"@type": "prov:Entity"},
	       "actor3":         {"@type": "prov:Agent"},
	       "do3":            {"@type": "prov:Activity"},	       
	       "wdf3":           {"@type": "provext:WasDerivedFromPlus"},
	       "wgb3":           {"@type": "prov:WasGeneratedBy"},	       
	       "waw3":           {"@type": "prov:WasAssociatedWith"}},

    "query": [
	"prefix ln <https://plead-project.org/ns/loan#>",
	"prefix sk <https://scikit-learn.org/stable/modules/generated/sklearn.>",
	"prefix ex <http://example/org>",

	"select * from waw a prov:WasAssociatedWith",
	"from actor a prov:Agent",
	" join waw.agent = actor.id",
	"from do a prov:Activity",
	" join waw.activity=do.id",

	"from wgb a prov:WasGeneratedBy",
	" join do.id=wgb.activity",
	"from pipeline a prov:Entity",
	" join wgb.entity=pipeline.id",
	"from wdf a prov:WasDerivedFrom",
	" join pipeline.id=wdf.usedEntity",
	"from recommendation a prov:Entity",
	" join wdf.generatedEntity=recommendation.id",


	"from wdf2 a prov:WasDerivedFrom",
	" join pipeline.id=wdf2.generatedEntity",
	"from pipeline2 a prov:Entity",
	" join wdf2.usedEntity=pipeline2.id",
	"from wgb2 a prov:WasGeneratedBy",
	" join pipeline2.id=wgb2.entity",
	"from do2 a prov:Activity",
	" join wgb2.activity=do2.id",

	"from waw2 a prov:WasAssociatedWith",
	" join do2.id=waw2.activity",
	"from actor2 a prov:Agent",
	" join waw2.agent = actor2.id",


	"where actor[prov:type]          >= 'prov:Person'",
	"  and recommendation[prov:type] >= 'ln:AutomatedLoanRecommendation'",
	"  and do [prov:type]            >= 'ln:PipelineApproval'",
	"  and pipeline [prov:type]      >= 'sk:pipeline.Pipeline'",
	"  and pipeline2[prov:type]      >= 'sk:pipeline.Pipeline'",
	"  and do2[prov:type]            >= 'ex:DataFitting'"

    ],

    "where": [{"@object":    "recommendation",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "ln:AutomatedLoanRecommendation"},

	      {"@object":    "pipeline2",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "sk:pipeline.Pipeline"},
	      {"@object":    "actor2",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "prov:Person"},
	      {"@object":    "do2",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "ln:PipelineApproval"},	      

	      {"@object":    "pipeline3",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "sk:pipeline.Pipeline"},
	      {"@object":    "actor3",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "prov:Person"},
	      {"@object":    "do3",
	       "@property":  "prov:type",
	       "@function":  "includesQualifiedName",
	       "@arg1":      "ln:DataFitting"},	      
	      
	      {"@object":    "waw2",
	       "@field":     "activity",
	       "@function":  "equals",
	       "@object1":   "do2",
	       "@field1":    "id"},
	      {"@object":    "waw2",
               "@field":     "agent",
	       "@function":  "equals",
	       "@object1":   "actor2",
	       "@field1":    "id"},

	      
	      {"@object":    "wgb2",
	       "@field":     "activity",
	       "@function":  "equals",
	       "@object1":   "do2",
	       "@field1":    "id"},
	      {"@object":    "wgb2",
               "@field":     "entity",
	       "@function":  "equals",
	       "@object1":   "pipeline2",
	       "@field1":    "id"},
	      
	      {"@object":    "wdf2",
	       "@field":     "usedEntity",
	       "@function":  "equals",
	       "@object1":   "pipeline2",
	       "@field1":    "id"},
	      {"@object":    "wdf2",
               "@field":     "generatedEntity",
	       "@function":  "equals",
	       "@object1":   "recommendation",
	       "@field1":    "id"},


	      {"@object":    "waw3",
	       "@field":     "activity",
	       "@function":  "equals",
	       "@object1":   "do3",
	       "@field1":    "id"},
	      {"@object":    "waw3",
               "@field":     "agent",
	       "@function":  "equals",
	       "@object1":   "actor3",
	       "@field1":    "id"},

	      
	      {"@object":    "wgb3",
	       "@field":     "activity",
	       "@function":  "equals",
	       "@object1":   "do3",
	       "@field1":    "id"},
	      {"@object":    "wgb3",
               "@field":     "entity",
	       "@function":  "equals",
	       "@object1":   "pipeline3",
	       "@field1":    "id"},
	      
	      {"@object":    "wdf3",
	       "@field":     "usedEntity",
	       "@function":  "equals",
	       "@object1":   "pipeline3",
	       "@field1":    "id"},
	      {"@object":    "wdf3",
               "@field":     "generatedEntity",
	       "@function":  "equals",
	       "@object1":   "recommendation",
	       "@field1":    "id"}],    

    "sentence": {
	"type": "clause",
	"subject": {"type": "noun_phrase",
		    "head": "responsibility",
		    "post-modifiers": [
			{"type": "preposition_phrase",
			 "preposition": "for",
			 "noun":  {"type":"noun_phrase",
				   "determiner": "the",
				   "head": {"@object": "recommendation",
					    "@field":  "id",
					    "@function": "noun+localname",
					    "@arg1": "recommendation"}}}
		    ]},
	"verb": "is",
	"modifiers": [{"type": "preposition_phrase",
		       "preposition": "with",
		       "noun":  {"type":"noun_phrase",
				 "determiner": "the",
				 "head": {"@object": "actor",
