{
    "var": {
        "pipeline": [
            {
                "@id": "file:*",
                "@type": "xsd:string",
                "@examplar": "file/pipeline678.csv",
                "@documentation": "The pipeline resulting from the training",
                "@input": "false",
                "@output": "true",
                "@sql.table": "file"
            }
        ],
        "training_dataset": [
            {
                "@id": "file:*",
                "@type": "xsd:string",
                "@examplar": "file/dataset.csv",
                "@documentation": "The dataset used int he training",
                "@input": "compulsory",
                "@output": "false",
                "@sql.table": "file"
            }
        ],
        "organization": [
            {
                "@id": "ag:*",
                "@type": "xsd:int",
                "@documentation": "An organization aiming managing the ML pipeline",
                "@input": "optional"
            }
        ],
        "engineer": [
            {
                "@id": "ag:*",
                "@type": "xsd:int",
                "@documentation": "The engineer developing the ML pipeline",
                "@input": "optional"
            }
        ],
        "training": [
            {
                "@id": "act:*",
                "@type": "xsd:int",
                "@documentation": "The activity of transforming the file",
                "@output": "true",
                "@sql.table": "activity"
            }
        ],
        "path": [
            [
                {
                    "@value": "*",
                    "@type": "xsd:string",
                    "@documentation": "The actual path of the file",
                    "@input": "optional"
                }
            ]
        ],
        "time": [
            [
                {
                    "@value": "*",
                    "@type": "xsd:dateTime",
                    "@documentation": "Time when the pipeline is created",
                    "@input": "optional",
                    "@sql.type":  "timestamptz"
                }
            ]
        ],
        "start": [
            [
                {
                    "@value": "*",
                    "@type": "xsd:dateTime",
                    "@documentation": "Time when the transformation starts",
                    "@input": "optional",
                    "@sql.type":  "timestamptz"
                }
            ]
        ],
        "end": [
            [
                {
                    "@value": "*",
                    "@type": "xsd:dateTime",
                    "@documentation": "Time when the transformation ends",
                    "@input": "optional",
                    "@sql.type":  "timestamptz"
                }
            ]
        ]
    },
    "context": {
        "ag":  "${PROV_API}/live/agent/",
        "act": "${PROV_API}/live/activity/",
        "file": "${PROV_API}/live/file/"
    },
    "template": "TODO template",
    "@documentation": "TODO doc."
}
