{
	"group": "org.openprovenance.prov",
	"name": "template_library",
	"destination": "target",
	"description": "PROV templates for plead ML pipeline",
	"version": "0.1.0",
	"init_package": "org.openprovenance.prov.template.library",
	"logger": "Logger",
	"templateBuilders": "TemplateBuilders",
	"tableConfigurator": "TableConfigurator",
	"beanProcessor": "BeanProcessor",
	"logger_package": "org.openprovenance.prov.template.library.logger",
	"configurator_package": "org.openprovenance.prov.template.library.plead.client.configurator",
	"script": "tmp_run",
	"script_dir": "target/bin",
	"sbean": true,
	"jsweet": true,
	"jsonschema": "schema.json",
	"documentation": "doc.html",
	"sqlFile": "prov-template-library.sql",
	"templates" : [

		{
			"@type": "simple",
			"name": "plead_transforming",
			"template": "target/prov-template-library/org/openprovenance/prov/templates/plead/plead-transforming.provn",
			"package": "org.openprovenance.prov.template.library.plead",
			"bindings": "src/main/resources/bindings/org/openprovenance/prov/templates/plead/plead-transforming.json"
		},

		{
			"@type": "simple",
			"name": "plead_filtering",
			"template": "target/prov-template-library/org/openprovenance/prov/templates/plead/plead-filtering.provn",
			"package": "org.openprovenance.prov.template.library.plead",
			"bindings": "src/main/resources/bindings/org/openprovenance/prov/templates/plead/plead-filtering.json"
		},
		{
			"@type": "simple",
			"name": "plead_training",
			"template": "target/prov-template-library/org/openprovenance/prov/templates/plead/plead-training.provn",
			"package": "org.openprovenance.prov.template.library.plead",
			"bindings": "src/main/resources/bindings/org/openprovenance/prov/templates/plead/plead-training.json"
		}

	],
	"sql.tables":  {
		"activity": {
			"id": "SERIAL",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"impact": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"aspect": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"environmentType": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"policy": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"objective": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"target": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"reports": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},
		"method": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"methods": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"progress": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},

		"obligation": {
			"id": "SERIAL",
			"serial": "INT",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		}


	}
}
