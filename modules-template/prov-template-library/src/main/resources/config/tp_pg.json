{
    "group": "org.openprovenance",
    "name": "template_library",
    "destination": "target/",
    "description": "Pokemon Go simulation",
    "version": "0.1.0",
    "root_package": "org.openprovenance.prov.template.library.pg",
    "script": "pokemon_run",
    "script_dir": "target/bin",
    "python_dir": "target/generated-python",
    "integrator": true,
    "jsweet": true,
    "jsonschema": "schema-pg.json",
    "sqlFile": "prov-template-library-pg.sql",
    "documentation": "doc-pg.html",
    "templates" : [

        {
            "@type": "simple",
            "fullyQualifiedName": "org.openprovenance.prov.template.library.pg.PgCapturing",
            "name": "pg_capturing",
            "package": "org.openprovenance.prov.template.library.pg",
            "template": "org/openprovenance/prov/templates/pg/pg-capturing",
            "cbindings": "org/openprovenance/prov/templates/pg/pg-capturing.json"
        },
        {
            "@type": "simple",
            "fullyQualifiedName": "org.openprovenance.prov.template.library.pg.PgBallcollecting",
            "name": "pg_ballcollecting",
            "package": "org.openprovenance.prov.template.library.pg",
            "template": "org/openprovenance/prov/templates/pg/pg-ballcollecting",
            "cbindings": "org/openprovenance/prov/templates/pg/pg-ballcollecting.json"
        }
    ]
}
