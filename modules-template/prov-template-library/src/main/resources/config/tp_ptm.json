{
	"group": "org.openprovenance.prov",
	"name": "template_library",
	"destination": "target",
	"description": "PROV templates for prov template management",
	"version": "2.2.3-SNAPSHOT",
	"root_package": "org.openprovenance.prov.template.library.ptm",
	"script": "ptm_run",
	"script_dir": "target/bin",
	"python_dir": "target/generated-python",
	"integrator": true,
	"jsweet": true,
	"jsonschema": "schema-ptm.json",
	"documentation": "doc-ptm.html",
	"sqlFile": "prov-template-library-ptm.sql",
	"templates" : [


		{
			"@type": "simple",
			"name": "ptm_expanding",
            "fullyQualifiedName": "org.openprovenance.prov.template.library.ptm.PtmExpanding",
			"template": "org/openprovenance/prov/templates/ptm/ptm-expanding",
			"package": "org.openprovenance.prov.template.library.ptm",
			"cbindings": "org/openprovenance/prov/templates/ptm/ptm-expanding.json"
		},

		{
			"@type": "simple",
			"name": "ptm_merging",
            "fullyQualifiedName": "org.openprovenance.prov.template.library.ptm.PtmMerging",
			"template": "org/openprovenance/prov/templates/ptm/ptm-merging",
			"package": "org.openprovenance.prov.template.library.ptm",
			"cbindings": "org/openprovenance/prov/templates/ptm/ptm-merging.json"
		},
		{
			"@type": "simple",
			"name": "ptm_mexpanding",
            "fullyQualifiedName": "org.openprovenance.prov.template.library.ptm.PtmMexpanding",
			"template": "org/openprovenance/prov/templates/ptm/ptm-mexpanding",
			"package": "org.openprovenance.prov.template.library.ptm",
			"cbindings": "org/openprovenance/prov/templates/ptm/ptm-mexpanding.json"
		}

	],
	"sql.tables":  {
		"activity": {
			"id": "SERIAL",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		},
		"file": {
			"id": "SERIAL",
			"created_at": "timestamp with time zone NOT NULL DEFAULT NOW()"
		}

	}
}
