            "output": "org/openprovenance/templates/generic/binaryop",
            "bindings": "binaryop.json",
            "formats": ["provn", "png", "svg"],
            "hasProvenance": "org/openprovenance/templates/generic/prov/prov-binaryop.prov-csv"
        }
    ]


