@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dot: <http://openprovenance.org/provtoolbox/dot/ns#> .
@prefix provext: <http://openprovenance.org/prov/extension#> .
@prefix vargen: <http://openprovenance.org/vargen#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <https://example.org/ns/#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix tmpl: <http://openprovenance.org/tmpl#> .
@prefix var: <http://openprovenance.org/var#> .


var:consent1 a prov:Entity , ex:ConsentGranted ;
	ex:data var:dataItem1 ;
	ex:purpose var:purpose1 .

var:purpose1 a prov:Entity , ex:Purpose ;
	ex:activity ex:DeterminingIfQualifyingForLoan .

var:dataItem1 a prov:Entity , ex:DemographicIdentity .

var:processingApplication a prov:Activity , ex:DeterminingIfQualifyingForLoan .

var:consenting a prov:Activity ;
	prov:used var:purpose1 , var:dataItem1 .

var:consent1 prov:wasGeneratedBy var:consenting .

var:decision a prov:Entity , ex:LoanApplicationDecision ;
	prov:wasDerivedFrom var:dataItem1 ;
	prov:wasGeneratedBy var:processingApplication .

var:processingApplication prov:used var:dataItem1 .

var:customer a prov:Agent .

var:bank a prov:Agent .

var:purpose1 prov:wasAttributedTo var:bank .

var:checking a prov:Activity ;
	prov:used var:purpose1 , var:consent1 .

var:permission prov:wasGeneratedBy var:checking ;
	prov:wasDerivedFrom var:purpose1 , var:consent1 ;
	a prov:Entity ;
	prov:value ex:OK .

var:processingApplication prov:wasStartedBy var:permission .

_:blank1 a prov:Association ;
	prov:agent var:bank .

var:processingApplication prov:qualifiedAssociation _:blank1 .

_:blank1 prov:hadPlan var:purpose1 .

var:dataItem1 prov:wasAttributedTo var:customer .

var:consent1 prov:wasAttributedTo var:customer ;
	prov:wasDerivedFrom var:dataItem1 .
