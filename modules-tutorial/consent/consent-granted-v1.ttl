@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dot: <http://openprovenance.org/provtoolbox/dot/ns#> .
@prefix provext: <http://openprovenance.org/prov/extension#> .
@prefix vargen: <http://openprovenance.org/vargen#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <https://example.org/ns/#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix tmpl: <http://openprovenance.org/tmpl#> .
@prefix var: <http://openprovenance.org/var#> .


var:consent2 a prov:Entity , ex:ConsentGranted ;
	ex:data var:dataItem1 ;
	ex:purpose var:purpose2 .

var:purpose2 a prov:Entity , ex:Purpose ;
	ex:activity ex:DeterminingIfLoanDecisionIsFair .

var:dataItem1 a prov:Entity , ex:DemographicIdentity .

var:consenting a prov:Activity ;
	prov:used var:purpose2 , var:dataItem1 .

var:consent2 prov:wasGeneratedBy var:consenting .

var:customer a prov:Agent .

var:bank a prov:Agent .

var:dataItem1 prov:wasAttributedTo var:customer .

var:consent2 prov:wasAttributedTo var:customer .

var:purpose2 prov:wasAttributedTo var:bank .

var:auditing a prov:Activity , ex:DeterminingIfLoanDecisionIsFair .

_:blank1 a prov:Association ;
	prov:agent var:bank .

var:auditing prov:qualifiedAssociation _:blank1 .

_:blank1 prov:hadPlan var:purpose2 .

var:report a prov:Entity , var:FairnessReport ;
	prov:wasGeneratedBy var:auditing .

var:checking a prov:Activity ;
	prov:used var:purpose2 , var:consent2 .

var:anonymising prov:wasStartedBy var:permission .

var:permission prov:wasGeneratedBy var:checking .

var:anonymising a prov:Activity ;
	prov:used var:dataItem1 .

var:anonymised1 prov:wasGeneratedBy var:anonymising .

var:permission a prov:Entity ;
	prov:value ex:OK .

var:anonymised1 a prov:Entity , ex:AnonymisedData .

var:auditing prov:used var:anonymised1 .

var:anonymised1 prov:wasDerivedFrom var:dataItem1 .

var:consent2 prov:wasDerivedFrom var:dataItem1 .

var:report prov:wasDerivedFrom var:anonymised1 .
